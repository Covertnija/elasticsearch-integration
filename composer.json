{
    "name": "covertnija/elasticsearch-integration",
    "description": "Symfony bundle for Elasticsearch integration with round-robin load balancing",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "elasticsearch", "bundle", "search", "load-balancing"],
    "authors": [
        {
            "name": "Domas Vysockis",
            "email": "leecher582@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "elasticsearch/elasticsearch": "^9.2",
        "psr/log": "^3.0",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "EV\\ElasticsearchIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EV\\ElasticsearchIntegration\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
